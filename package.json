{
  "name": "ai-powered-salesperson-chat-system",
  "version": "1.0.0",
  "description": "An AI-powered chat system for salespeople to assist customers and generate quotes",
  "main": "src/backend/server.ts",
  "scripts": {
    "start": "node dist/backend/server.js",
    "dev": "nodemon src/backend/server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "client": "cd src/frontend && npm start",
    "server": "nodemon src/backend/server.ts",
    "dev:full": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5",
    "axios": "^0.21.1",
    "mongoose": "^5.13.2",
    "redis": "^3.1.2",
    "socket.io": "^4.1.3",
    "jsonwebtoken": "^8.5.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "prettier": "^2.3.2",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "nodemon": "^2.0.12",
    "concurrently": "^6.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-powered-salesperson-chat-system.git"
  },
  "keywords": [
    "ai",
    "chat",
    "sales",
    "quotes",
    "typescript",
    "react",
    "node"
  ],
  "author": "Your Name",
  "license": "MIT"
}